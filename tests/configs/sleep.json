{
  "clean": "rm -f sleep.sh",
  "build": "echo sleep {sleep_duration} > sleep.sh",
  "run": "bash ./sleep.sh",
  "args": [
    {
      "name": "sleep_duration",
      "env": {"{name}":  "{SELF}"},
      "constraint": "{SELF} * {SELF} > .09",
      "range": {
        "continuous": [0.0, 0.5]
      }
    }
  ]
}